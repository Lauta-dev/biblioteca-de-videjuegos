<div id="inputs_de_registro">
  <input
    type="text"
    name="game"
    id="game"
    placeholder="Juego - Ejem: Call Of Duty"
    autofocus
  />

  <input
    type="text"
    name="image"
    id="image"
    placeholder="URL - Ejem https://example.com/tes-skyrim/carrera-blanca.png"
  />

  <input
    type="number"
    name="created_by"
    id="created_by"
    placeholder="ID del empleado"
  />
</div>

<button type="submit" class="btn-desactivado" disabled>Guardar Info</button>
