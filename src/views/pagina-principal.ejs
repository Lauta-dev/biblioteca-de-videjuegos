<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include("./parties/header.ejs") %>
    <title>Landing page</title>
  </head>
  <body>
    <div style="display: flex; width: 200px; height: 200px">
      <% results.map(({ juego, id, img }) => { %> <% if(!img) { %>
      <img
        src="https://gamefaqs.gamespot.com/a/box/0/1/3/435013_front.jpg"
        alt="a"
        style="width: auto"
      />
      <% } else { %>
      <div>
        <img src="<%= img %>" alt="img" style="width: 100px" />
      </div>
      <% } %> <% }) %>
    </div>
  </body>
</html>
